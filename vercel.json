{
    "builds": [{
        "src": "travel/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "travel/wsgi.py"
        }
    ],

    
    "env": {
        "mysql_host": "@mysql_host",
        "mysql_user": "@mysql_user",
        "mysql_password": "@mysql_password",
        "mysql_database": "@mysql_database"
    }
      
}